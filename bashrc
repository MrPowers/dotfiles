export PS1="\W \$ " # use a lower case w for the entire directory cookie trail
alias ll='ls -ahlF'
alias ruby_games="cd /Users/matthewpowers/Documents/ruby_games/"
alias documents="cd ~/Documents"
export PATH="$HOME/bin:$PATH
GREP_OPTIONS="--color=always";export GREP_OPTIONS
set -o noclobber
[ -f /opt/boxen/env.sh ] && source /opt/boxen/env.sh
